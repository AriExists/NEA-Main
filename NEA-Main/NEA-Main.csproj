    </Resource>
    <Resource Include="Assets\phone.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\server.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
